generator client {
  provider = "prisma-client-js"
  output   = "src//generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model chat_messages {
  id         String                  @id(map: "PK_40c55ee0e571e268b0d3cd37d10") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  role       chat_messages_role_enum @default(user)
  content    String
  created_at DateTime                @default(now()) @db.Timestamp(6)
  chat_id    String?                 @db.Uuid
  chats      chats?                  @relation(fields: [chat_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_9f5c0b96255734666b7b4bc98c3")
}

model chats {
  id            String          @id(map: "PK_0117647b3c4a4e5ff198aeb6206") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  title         String          @db.VarChar(64)
  created_at    DateTime        @default(now()) @db.Timestamp(6)
  updated_at    DateTime        @default(now()) @db.Timestamp(6)
  chat_messages chat_messages[]
}

model migrations {
  id        Int    @id(map: "PK_8c82d7f526340ab734260ea46be") @default(autoincrement())
  timestamp BigInt
  name      String @db.VarChar
}

enum chat_message_role_enum {
  user
  assistant
  system
}

enum chat_messages_role_enum {
  user
  assistant
  system
}
